<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/assets/css/settings.css">
    <link rel="shortcut icon" href="/assets/images/favi/favicon.png" type="image/x-icon">
    <title>KevinServ DB - Settings</title>
</head>
<body>

<header>
    <nav>
        <ul>
            <li><a draggable="false" href="/"><img draggable="false" class="logo" src="/assets/images/KS-logo.png" alt="logo"></a></li>
            <li><a draggable="false" href="/settings">Settings</a></li>
            <li><a draggable="false" href="/dashboard">Dashboard</a></li>
        </ul>
    </nav>
</header>
<main class="flex-c">
    <div class="container">
        <article class="title">
            <a href="/dashboard" class="transparent-c-btn flex-c btn"><img height="22px" width="22px" src="/assets/svg/back-arrow-icon.svg"></a>
            <h3>Settings</h3>
        </article>
        <article class="main">
            <section class="menu">
                <ul>
                    <li class="btn-s2 btn">Invites</li>
                    <li class="btn-s2 btn">Log</li>
                    <li class="btn-s2 btn">Account</li>
                </ul>
            </section>

            <section class="content">
                <div class="log">
                    <main>
                        <% logData.reverse().forEach((data) => { %>
    
                            <article>
                                <p class="sub">Client</p> <p> <%= data.client %> </p>
                                <p class="sub">Data</p> <p> <%= data.date %> </p>
                                <span><p class="sub">IP: </p> <p> <%= data.ip%> </p></span>
                            </article>
    
                        <% }) %>   
                    </main>
                    <div>
                        <a class="flex-c btn btn-s3" href="/delete/log"><img height="16px" width="16px" src="/assets/svg/bin-icon-white.svg">Log</a>
                    </div>
                </div>
            </section>
        </article>
    </div>
</main>

<script src="/assets/js/settings.js"></script>
</body>
</html>